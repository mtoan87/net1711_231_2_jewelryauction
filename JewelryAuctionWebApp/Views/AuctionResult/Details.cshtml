@model JewelryAuctionData.Models.AuctionResult

@{
    ViewBag.Title = "AuctionResult Details";
}

<h2>Auction Result Details</h2>

<dl class="row">
    <dt class="col-sm-2">Auction Result ID</dt>
    <dd class="col-sm-10">@Model.AuctionResultId</dd>

    <dt class="col-sm-2">Jewelry ID</dt>
    <dd class="col-sm-10">@Model.JewelryId</dd>

    <dt class="col-sm-2">Customer ID</dt>
    <dd class="col-sm-10">@Model.CustomerId</dd>
    

    <dt class="col-sm-2">Join Auction ID</dt>
    <dd class="col-sm-10">@Model.JoinAuctionId</dd>

    <dt class="col-sm-2">Bid ID</dt>
    <dd class="col-sm-10">@Model.BidId</dd>

    <dt class="col-sm-2">Date</dt>
    <dd class="col-sm-10">@Model.Date</dd>

    <dt class="col-sm-2">Price</dt>
    <dd class="col-sm-10">@Model.Price</dd>

    <dt class="col-sm-2">Detail</dt>
    <dd class="col-sm-10">@Model.Detail</dd>

    <dt class="col-sm-2">Description</dt>
    <dd class="col-sm-10">@Model.Description</dd>

    <dt class="col-sm-2">Starting Price</dt>
    <dd class="col-sm-10">@Model.StartingPrice</dd>

    <dt class="col-sm-2">Status</dt>
    <dd class="col-sm-10">@Model.Status</dd>


</dl>

<div>
    <a asp-action="Index" class="btn btn-primary">Back to List</a>
</div>
