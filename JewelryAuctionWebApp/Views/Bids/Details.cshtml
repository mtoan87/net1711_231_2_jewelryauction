@model JewelryAuctionData.Models.Bid

@{
    ViewBag.Title = "Bid Details";
}

<h2>Bid Details</h2>

<dl class="row">
    <dt class="col-sm-2">Bid ID</dt>
    <dd class="col-sm-10">@Model.BidId</dd>

    <dt class="col-sm-2">Customer ID</dt>
    <dd class="col-sm-10">@Model.CustomerId</dd>

    <dt class="col-sm-2">Join Auction ID</dt>
    <dd class="col-sm-10">@Model.JoinAuctionId</dd>

    <dt class="col-sm-2">Bid Amount</dt>
    <dd class="col-sm-10">@Model.BidAmount</dd>

    <dt class="col-sm-2">Date Time</dt>
    <dd class="col-sm-10">@Model.DateTime</dd>

    <dt class="col-sm-2">Jewelry ID</dt>
    <dd class="col-sm-10">@Model.JewelryId</dd>

    <dt class="col-sm-2">Bidder Name</dt>
    <dd class="col-sm-10">@Model.BidderName</dd>

    <dt class="col-sm-2">Join Auction Name</dt>
    <dd class="col-sm-10">@Model.JoinAuctionName</dd>

    <dt class="col-sm-2">Join Auction Description</dt>
    <dd class="col-sm-10">@Model.JoinAuctionDescription</dd>

    <dt class="col-sm-2">Bid Status</dt>
    <dd class="col-sm-10">@Model.BidStatus</dd>

    <dt class="col-sm-2">Is Wining</dt>
    <dd class="col-sm-10">@Model.IsWining</dd>
</dl>

<div>
    <a asp-action="Index" class="btn btn-primary">Back to List</a>
</div>
